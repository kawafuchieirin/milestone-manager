# AWS設定
aws_region          = "ap-northeast-1"
project_name        = "milestone-manager"
environment         = "dev"
dynamodb_table_name = "milestone-manager"

# Cognito コールバックURL
cognito_callback_urls = [
  "http://localhost:5173/"
]

# Cognito ログアウトURL
cognito_logout_urls = [
  "http://localhost:5173/login"
]

# CORS許可オリジン
cors_allowed_origins = [
  "http://localhost:5173",
  "http://localhost:3000"
]

# カスタムドメイン（オプション）
# frontend_domain     = "app.example.com"
# acm_certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/xxx"
